{{#each details.root.prefixdata as |result index|}}
  {{#if (gt index 0)}}
    <hr>
  {{/if}}
  <h1 class="p-title">
    {{fa-icon "phone-rotary" fixedWidth=true}}  Prefix Details
  </h1>
  {{#if result.exch}}
    <div>
      <span class="p-key">Exch Code:</span>
      <span class="p-value">{{result.exch}}</span>
    </div>
  {{/if}}
  {{#if result.rc}}
    <div>
      <span class="p-key">Rate Center Name:</span>
      <span class="p-value">{{result.rc}}</span>
    </div>
  {{/if}}
  {{#if result.region}}
    <div>
      <span class="p-key">Region:</span>
      <span class="p-value">{{result.region}}</span>
    </div>
  {{/if}}
  {{#if result.switch}}
    <div>
      <span class="p-key">CLLI code:</span>
      <span class="p-value">{{result.switch}}</span>
    </div>
  {{/if}}
  {{#if result.switchname}}
    <div>
      <span class="p-key">Switch Name:</span>
      <span class="p-value">{{result.switchname}}</span>
    </div>
  {{/if}}
  {{#if result.switchtype}}
    <div>
      <span class="p-key">Switch Type:</span>
      <span class="p-value">{{result.switchtype}}</span>
    </div>
  {{/if}}
  {{#if result.ocn}}
    <div>
      <span class="p-key">OCN (Operating Company Number) Code:</span>
      <span class="p-value">{{result.ocn}}</span>
    </div>
  {{/if}}
  {{#if result.company-name}}
    <div>
      <span class="p-key">Company Name:</span>
      <span class="p-value">{{result.company-name}}</span>
    </div>
  {{/if}}
  {{#if result.company-type}}
    <div>
      <span class="p-key">Company Type:</span>
      <span class="p-value">{{result.company-type}}</span>
    </div>
  {{/if}}
  {{#if result.rc-lat}}
    <div>
      <span class="p-key">Rate Center Latitude:</span>
      <span class="p-value">{{result.rc-lat}}</span>
    </div>
  {{/if}}
  {{#if result.rc-lon}}
    <div>
      <span class="p-key">Rate Center Longitude:</span>
      <span class="p-value">{{result.rc-lon}}</span>
    </div>
  {{/if}}
  {{#if result.udate}}
    <div>
      <span class="p-key">Last Updated:</span>
      <span class="p-value">{{result.udate}}</span>
    </div>
  {{/if}}
{{/each}}